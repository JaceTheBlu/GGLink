{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "1.0",
    
    "homepage_url": "https://github.com/JaceTheBlu/GGLink",
    "default_locale": "en",
    
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "96": "icons/icon96.png"
    },

    "permissions": [
        "contextMenus",
        "activeTab",
        "clipboardWrite"
    ],
    
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}